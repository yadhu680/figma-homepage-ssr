#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/dist/bin/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/dist/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/dist/bin/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/dist/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules:/mnt/e/figma-homepage-ssr/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../.pnpm/next@15.0.0_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/next/dist/bin/next" "$@"
fi
