{
  "name": "figma-homepage-ssr",
  "version": "1.0.0",
  "private": true,
  "description": "Next.js 15 SSR homepage based on Figma design using React 19 and MUI v7",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/material-nextjs": "^7.0.0",
    "framer-motion": "^12.23.24",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@types/node": "^22.8.1",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "next>react": "$react",
      "next>react-dom": "$react-dom"
    }
  },
  "packageManager": "pnpm@10.17.1"
}
